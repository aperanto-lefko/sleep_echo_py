}
QPushButton:hover {
    background-color:rgb(60, 118, 184);  /* Цвет фона при наведении */
}

QPushButton:pressed {
    background-color: rgb(83, 163, 255);  /* Цвет фона при нажатии */
}
QLabel {
    color: white;  /* Белый цвет текста */
}
QLineEdit {
    background-color: rgb(62, 62, 62);  /* Темный фон */
    color: white;               /* Белый текст */
    border: 1px solid #555555;  /* Граница */
    padding: 4px;
    border-radius: 3px;
}
QTableWidget {
    color: white;  /* цвет текста во всех ячейках */
    background-color:rgb(62, 62, 62);  /* фон таблицы (опционально) */
}
QComboBox {
    background-color:rgb(62, 62, 62);   /* фон поля */
    color: white;                /* цвет текста */
    border: 1px solid #555555;   /* рамка */
    padding: 4px 20px 4px 8px;   /* отступы */
    border-radius: 4px;
}
QComboBox QAbstractItemView {
    background-color: rgb(62, 62, 62); /* фон выпадающего списка */
    color: white;              /* цвет текста в списке */
    border: 1px solid #555555;
    selection-background-color: #007ACC; /* цвет подсветки выбранного */
    selection-color: white;
}
QHeaderView::section {
    background-color:rgb(62, 62, 62); /* фон заголовков */
    color: white;               /* цвет текста заголовков */
    padding: 4px;
    border: 1px solid #555555;
}
QHeaderView:horizontal::section {
    background-color: rgb(62, 62, 62);
    color: white;
}

QHeaderView:vertical::section {
    background-color: rgb(62, 62, 62);
    color: white;
}</string>
