QTabWidget::pane {
    border: 1px solid #444444; /* рамка вокруг области вкладок */
    background: #222222;       /* фон панели вкладок */
}

QTabBar::tab {
    background: rgb(62, 62, 62);;        /* цвет фона невыбранных вкладок */
    color: white;               /* цвет текста */
    padding: 8px 16px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border: 1px solid #444444;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background: rgb(74, 144, 226);        /* цвет выбранной вкладки */
    color: white;
    border: 1px solid #007ACC;
    border-bottom-color: #222222; /* чтобы выделенная вкладка визуально соединялась с панелью */
}

QTabBar::tab:hover {
    background: #005A9E;        /* цвет при наведении */
}</string>
